@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>
<html>
<head>
    <title>Calendar</title>

    <h5>@ViewBag.StMsg</h5>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
   
    <style>
        #panel, #flip {
            width: 350px;
            padding: 5px;
            text-align: center;
            background-color: #529bc4;
            border: solid 1px #c3c3c3;
        }

        #panel {
            padding: 50px;
            display: none;
        }
    </style>
    <style>
        body {
            background-color: #fffbd3;
        }
    </style>
</head>

<body>
    <p id="cal">@Session["Position"]</p> 
    <br />  
    <div style="height:700px;width:900px;margin:0 auto">
        @Html.Raw(Model.Render())
    </div>
</body>
</html>
